Tabs.Event:AddSection("Kitsune")

local FrozenIsland = Tabs.Event:AddParagraph({
    Title = "Check Kitsune Island",
    Content = ""
})
spawn(function()
    while wait() do
        pcall(function()
            if game.Workspace["_WorldOrigin"].Locations:FindFirstChild("Frozen Dimension") then
                FrozenIsland:SetDesc('🟢: Frozen Dimension Spawning')
            else
                FrozenIsland:SetDesc('🔴: Frozen Dimension Not Found')
            end
        end)
    end
end)

