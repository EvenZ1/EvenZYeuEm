local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()
--------------------------------------------------------------------------------------------------------------------------------------------
local Window = Fluent:CreateWindow({
    Title = "Summer Hub",
    SubTitle = " By EvenZ",
    TabWidth = 160,
    Size = UDim2.fromOffset(520, 350),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Darker",
    MinimizeKey = Enum.KeyCode.LeftControl -- Used when theres no MinimizeKeybind
})

local Tabs = {
    Shop = Window:AddTab({ Title = "Shop", Icon = "diamond" }),
    Main = Window:AddTab({ Title = "Main Farm", Icon = "diamond" }),
    Main2 = Window:AddTab({ Title = "Sub Farm", Icon = "diamond" }),
    Setting = Window:AddTab({ Title = "Setting", Icon = "diamond" }),
    Item = Window:AddTab({ Title = "Get Item", Icon = "diamond" }),
    Status = Window:AddTab({ Title = "Status", Icon = "diamond" }),
    Teleport = Window:AddTab({ Title = "Teleport Island", Icon = "diamond" }),
    Race = Window:AddTab({ Title = "RaceV4-Mirage", Icon = "diamond" }),
    Event = Window:AddTab({ Title = "Sea Event", Icon = "diamond" }),
    Raid = Window:AddTab({ Title = "Raid - Material", Icon = "diamond" }),
    Misc = Window:AddTab({ Title = "Misc", Icon = "diamond" })
}
Window:SelectTab(1)
local Options = Fluent.Options

--// Close Ui

local ScreenGui1 = Instance.new("ScreenGui")
local ImageButton1 = Instance.new("ImageButton")
local UICorner = Instance.new("UICorner")

ScreenGui1.Name = "ImageButton"
ScreenGui1.Parent = game.CoreGui
ScreenGui1.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

ImageButton1.Parent = ScreenGui1
ImageButton1.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
ImageButton1.BorderSizePixel = 0
ImageButton1.Position = UDim2.new(0.120833337, 0, 0.0952890813, 0)
ImageButton1.Size = UDim2.new(0, 50, 0, 50)
ImageButton1.Draggable = true
ImageButton1.Image = "rbxassetid://109429001212695"
ImageButton1.MouseButton1Down:connect(function()
  game:GetService("VirtualInputManager"):SendKeyEvent(true,"LeftControl",false,game)
  game:GetService("VirtualInputManager"):SendKeyEvent(false,"LeftControl",false,game)
end)
UICorner.Parent = ImageButton1
                 

    
